<record type="audio/x-wav" name="temprecording" beep="true" dtmfterm="false" finalsilence="3s" maxtime="30s" >
<prompt>
<audio src="SAN-RECORDING">Por favor, diga su nombre</audio>
</prompt>
<prompt>
<audio src="SAN-RECORDING-B"/>
</prompt>
<prompt>Despues del beep</prompt>
<property name="other.property" value="20s" />
<property name="second.other.property" value="35s" />
<catch event="connection.disconnect.hangup">
<submit next="http://flowExecutionUrl/&amp;_eventId_hangup" method="post" />
</catch>
<catch event="com.nortel.ivr.record.notstored">
<submit next="http://flowExecutionUrl/&amp;_eventId_com.nortel.ivr.record.notstored" method="post" />
</catch>
<catch event="error.noresource">
<submit next="http://flowExecutionUrl/&amp;_eventId_noresource" method="post" />
</catch>
<catch event="error.unsupported.record">
<submit next="http://flowExecutionUrl/&amp;_eventId_recordunsupported" method="post" />
</catch>
<catch event="error">
<submit next="http://flowExecutionUrl/&amp;_eventId_error" method="post" />
</catch>
<filled>
<var name="duration" expr="temprecording$.duration" />
<var name="size" expr="temprecording$.size" />
<var name="termchar" expr="temprecording$.termchar" />
<var name="maxtime" expr="temprecording$.maxtime" />
<submit next="http://flowExecutionUrl/&amp;_eventId_recorded" namelist="temprecording duration size termchar maxtime" method="post" enctype="multipart/form-data" />
</filled>
</record>